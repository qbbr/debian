! ==================================================
! file:     ~/.Xdefaults
! author:   Sokolov Innokenty - qbbr.ru
! modified: November 13, 2009
! vim:enc=utf-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
! ==================================================

! xterm
! ==================================================
xterm*geometry:				80x25
xterm*faceName:				terminus:pixelsize=14
xterm*boldMode:				false
xterm*dynamicColors:		true
xterm*utf8:					2
xterm*eightBitInput:		true
xterm*saveLines:			1000
xterm*scrollTtyKeypress:	true
xterm*scrollTtyOutput:		false
xterm*scrollBar:			true
xterm*loginShell:			true
xterm*jumpScroll:			false
xterm*multiScroll:			true
xterm*toolBar:				true
xterm*charClass:			33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48

! colors
! ==================================================
*background:				rgb:11/11/11
*foreground:				rgb:ff/ff/ff
! Black
*color0:					rgb:00/00/00
*color8:					rgb:00/00/00
! Red
*color1:					rgb:bf/46/46
*color9:					rgb:f4/8a/8a
! Green
*color2:					rgb:67/b2/5f
*color10:					rgb:a5/d7/9f
! Yellow
*color3:					rgb:cf/c4/4e
*color11:					rgb:e1/da/84
! Blue
*color4:					rgb:51/60/83
*color12:					rgb:a2/bb/ff
! Mangenta
*color5:					rgb:ca/6e/ff
*color13:					rgb:e2/b0/ff
! Cyan 
*color6:					rgb:92/b2/f8
*color14:					rgb:ba/cd/f8
! White
*color7:					rgb:d5/d5/d5
*color15:					rgb:d5/d5/d5

! Xft
! ==================================================
Xft.dpi:					96
Xft.antialias:				true
Xft.hinting:				true
Xft.hintstyle:				full
Xft.rgba:					rgb

! urxvt
! ==================================================
urxvt*font:					xft:terminus:pixelsize=14
urxvt*boldFont:				xft:terminus:pixelsize=14
!urxvt*background:			#111111
!urxvt*foreground:			#ffffff
urxvt*scrollBar:			true
urxvt*scrollTtyKeypress:	true
urxvt*scrollWithBuffer:		false
urxvt*scrollTtyOutput:		false
urxvt*scrollstyle:			plain
urxvt*scrollBar_right:		true
urxvt*scrollColor:			#729FCF
urxvt*geometry:				100x30
urxvt*internalBorder:		5
urxvt*fading:				20
urxvt*shading:				80
urxvt*tintColor:			#111111
urxvt*inheritPixmap:		false
urxvt*depth:				24
urxvt*saveLines:			1000
urxvt*visualBell:			true
urxvt*cursorColor:			#729FCF
urxvt*secondaryScroll:		true
urxvt*mapAlert:				true
urxvt*utmpInhibit:			true
urxvt*urlLauncher:			/usr/bin/google-chrome
urxvt*matcher.button:		1
urxvt*background:			#191919
