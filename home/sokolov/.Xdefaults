! ==================================================
! file:     ~/.Xdefaults
! author:   Sokolov Innokenty - qbbr.ru
! modified: November 13, 2009
! vim:enc=utf-8:nu:ai:si:ts=4:sw=4:ft=xdefaults:
! ==================================================

! xterm
! ==================================================
xterm*geometry:				80x25
xterm*faceName:				terminus:pixelsize=14
xterm*boldMode:				false
xterm*dynamicColors:		true
xterm*utf8:					2
xterm*eightBitInput:		true
xterm*saveLines:			1000
xterm*scrollTtyKeypress:	true
xterm*scrollTtyOutput:		false
xterm*scrollBar:			true
xterm*loginShell:			true
xterm*jumpScroll:			false
xterm*multiScroll:			true
xterm*toolBar:				true
xterm*charClass:			33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48

! colors
! ==================================================
*background:				rgb:11/11/11
*foreground:				rgb:ff/ff/ff
! Black
*color0:					rgb:00/00/00
*color8:					rgb:00/00/00
! Red
*color1:					rgb:bf/46/46
*color9:					rgb:f4/8a/8a
! Green
*color2:					rgb:67/b2/5f
*color10:					rgb:a5/d7/9f
! Yellow
*color3:					rgb:cf/c4/4e
*color11:					rgb:e1/da/84
! Blue
*color4:					rgb:51/60/83
*color12:					rgb:a2/bb/ff
! Magenta
*color5:					rgb:ca/6e/ff
*color13:					rgb:e2/b0/ff
! Cyan 
*color6:					rgb:92/b2/f8
*color14:					rgb:ba/cd/f8
! White
*color7:					rgb:d5/d5/d5
*color15:					rgb:d5/d5/d5

! Xft
! ==================================================
Xft.dpi:					96
Xft.antialias:				true
Xft.hinting:				true
Xft.hintstyle:				full
Xft.rgba:					rgb

! urxvt
! ==================================================
urxvt*font:					xft:terminus:pixelsize=14
urxvt*boldFont:				xft:terminus:pixelsize=14
!urxvt*background:			#111111
!urxvt*foreground:			#ffffff
urxvt*scrollBar:			true
urxvt*scrollTtyKeypress:	true
urxvt*scrollWithBuffer:		false
urxvt*scrollTtyOutput:		false
urxvt*scrollstyle:			plain
urxvt*scrollBar_right:		true
urxvt*scrollColor:			#729FCF
urxvt*geometry:				100x30
urxvt*internalBorder:		5
urxvt*fading:				20
urxvt*shading:				80
urxvt*tintColor:			#111111
urxvt*inheritPixmap:		false
urxvt*depth:				24
urxvt*saveLines:			1000
urxvt*visualBell:			true
urxvt*cursorColor:			#729FCF
urxvt*secondaryScroll:		true
urxvt*mapAlert:				true
urxvt*utmpInhibit:			true
urxvt*urlLauncher:			/usr/bin/google-chrome
urxvt*matcher.button:		1
urxvt*background:			#111111


! xpdf
! ==================================================
xpdf*enableFreetype:		yes
xpdf*antialias:				yes
xpdf*foreground:			white
!xpdf*background:			black
xpdf*urlCommand:			/usr/bin/google-chrom %s


! xcalc
! ==================================================
xcalc*geometry:							200x275
xcalc.ti.bevel.background:				#729FCF
!xcalc.ti.bevel.screen.background:		#000000
!xcalc.ti.bevel.screen.DEG.background:	#000000
xcalc.ti.bevel.screen.DEG.foreground:	white
!xcalc.ti.bevel.screen.GRAD.background:	#000000
xcalc.ti.bevel.screen.GRAD.foreground:	white
!xcalc.ti.bevel.screen.RAD.background:	#000000
xcalc.ti.bevel.screen.RAD.foreground:	white
!xcalc.ti.bevel.screen.INV.background:	#000000
xcalc.ti.bevel.screen.INV.foreground:	Red
!xcalc.ti.bevel.screen.LCD.background:	#000000
xcalc.ti.bevel.screen.LCD.foreground:	white
xcalc.ti.bevel.screen.LCD.shadowWidth:	0
!xcalc.ti.bevel.screen.M.background:		#000000
xcalc.ti.bevel.screen.M.foreground:		white
!xcalc.ti.bevel.screen.P.background:		#000000
xcalc.ti.bevel.screen.P.foreground:		Yellow
xcalc.ti.Command.foreground:			White
xcalc.ti.Command.background:			#888888
xcalc.ti.button5.background:			#bf4646
xcalc.ti.button19.background:			#666666
xcalc.ti.button18.background:			#666666
xcalc.ti.button20.background:			#555555
!uncomment to change label on division button
!xcalc.ti.button20.label:      /
xcalc.ti.button25.background:			#555555
xcalc.ti.button30.background:			#555555
xcalc.ti.button35.background:			#555555
xcalc.ti.button40.background:			#67b25f
xcalc.ti.button22.background:			#516083
xcalc.ti.button23.background:			#516083
xcalc.ti.button24.background:			#516083
xcalc.ti.button27.background:			#516083
xcalc.ti.button28.background:			#516083
xcalc.ti.button29.background:			#516083
xcalc.ti.button32.background:			#516083
xcalc.ti.button33.background:			#516083
xcalc.ti.button34.background:			#516083
xcalc.ti.button37.background:			#516083
xcalc.ti.button38.background:			#516083
xcalc.ti.button39.background:			#516083


! xclock
! ==================================================
xclock*update:			1
xclock*analog:			false
xclock*foreground:		white
!xclock*background:		black
