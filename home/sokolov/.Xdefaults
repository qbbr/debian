xcalc.ti.button34.background:           #516083
xcalc.ti.button37.background:           #516083
xcalc.ti.button38.background:           #516083
xcalc.ti.button39.background:           #516083


! xclock
! ==================================================
xclock*update:          1
xclock*analog:          false
xclock*foreground:      white
!xclock*background:     black
